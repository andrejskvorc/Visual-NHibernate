<?xml version="1.0" encoding="utf-8" ?>
<log4net>
	<appender name="A1" type="log4net.Appender.TraceAppender">
		<layout type="log4net.Layout.PatternLayout">
			<conversionPattern value="%-4timestamp [%thread] %-5level %logger - %message%newline" />
		</layout>
	</appender>

	<root>
		<level value="ALL" />
		<appender-ref ref="A1" />
	</root>
</log4net>